{
  "name": "terceirizadas",
  "version": "0.1.16",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run-script prettier && npm run-script eslint",
      "pre-push": "npm run-script eslint"
    }
  },
  "lint-staged": {
    "*.{jsx,js,tsx,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "watchPathIgnorePatterns": [
      "src/components/Shareable/Avatar"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 1,
        "branches": 1,
        "functions": 1,
        "lines": 1
      }
    },
    "transformIgnorePatterns": [
      "node_modules/(?!axios)"
    ]
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^35.4.0",
    "@ckeditor/ckeditor5-react": "^5.0.6",
    "@khanacademy/react-multi-select": "^0.3.3",
    "@sentry/browser": "^7.42.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/typescript-estree": "^6.5.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "antd": "^5.22.4",
    "axios": "^1.6.2",
    "axios-mock-adapter": "^1.21.1",
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "date-fns": "^2.29.3",
    "eslint-plugin-typescript": "^0.14.0",
    "file-saver": "^2.0.5",
    "final-form": "^4.20.9",
    "final-form-arrays": "^3.0.2",
    "final-form-calculate": "^1.3.2",
    "html-to-react": "^1.5.1",
    "http-status-codes": "^2.2.0",
    "jest": "^29.7.0",
    "jquery.easing": "^1.4.1",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "locutus": "^2.0.16",
    "moment": "^2.29.4",
    "multiselect-react-dropdown": "^2.0.25",
    "patch-package": "^7.0.1",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.1",
    "react-bootstrap": "^2.9.1",
    "react-burgers": "^1.3.0",
    "react-chartjs-2": "^5.2.0",
    "react-collapse": "^5.1.1",
    "react-datepicker": "4.16.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "react-redux": "^7.2.9",
    "react-router-dom": "^6.21.2",
    "react-select": "^5.8.0",
    "react-text-mask": "^5.4.3",
    "react-toastify": "^9.1.2",
    "redux": "^4.2.1",
    "redux-form": "^8.3.9",
    "redux-form-input-masks": "^2.0.2",
    "redux-multi": "^0.1.12",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.4.2",
    "startbootstrap-sb-admin-2": "^4.1.4",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint": "eslint ./src/**/**/**/**/*.{jsx,js,tsx,ts}",
    "prettier": "prettier  --check src/**/**/**/*.{jsx,js,scss,tsx,ts}",
    "prettier-fix": "prettier  --write src/**/**/**/*.{jsx,js,scss,tsx,ts}",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {}
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.2",
    "babel-jest": "^29.7.0",
    "fetch-mock": "^8.3.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^12.5.0",
    "msw": "^0.35.0",
    "node-localstorage": "^1.3.1",
    "prettier": "^2.8.8",
    "react-scripts": "^5.0.1",
    "sass": "^1.83.0"
  },
  "setupFiles": [
    "./src/setupTests.js"
  ]
}
